# Optional environment variables for Docker deployment
# Copy this file to .env and update the values

# Tavily API Key (for AI search features)
# Get your free key at: https://tavily.com
TAVILY_API_KEY=

# Database password (change for production)
MYSQL_ROOT_PASSWORD=pass123

# Session secret (change for production)
SESSION_SECRET=your-secret-key-change-in-production

# AI Agent Configuration
LLM_PROVIDER=ollama
LLM_MODEL=llama3.1:8b
OLLAMA_BASE_URL=http://host.docker.internal:11434

